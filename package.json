{
  "name": "gatsby-layout-builder",
  "version": "0.5.0",
  "workspaces": [
    "demo",
    "gatsby-layout-builder"
  ],
  "private": true,
  "description": "Gatsby Layout Builder - SEO.",
  "author": "Milton Bolonha <miltonbolonha@gmail.com>",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-layout",
    "gatsby-builder",
    "gatsby-layout-builder",
    "gatsby-layout-builder",
    "layout-builder",
    "seo",
    "builder"
  ],
  "scripts": {
    "test": "echo \"No test specified\"",
    "build": "npm run build -w gatsby-layout-builder",
    "clean": "npm run clean -w demo",
    "develop": "npm run develop -w demo",
    "version": "./build-docs && git add docs",
    "coverage": "nyc --reporter=text npm run test"
  },
  "nyc": {
    "include": "./gatsby-layout-builder/**/*.js",
    "exclude": [
      "./gatsby-layout-builder/node_modules/**/*.js",
      "./demo",
      "./node_modules/**/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuktpls/gatsby-layout-builder.git"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "@nuktpls:registry": "https://npm.pkg.github.com"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "gatsby-layout-builder",
          "npmPublish": true,
          "tarballDir": "gatsby-layout-builder/dist"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "verifyConditions": [
    {
      "path": "semantic-release-verify-deps",
      "dependencies": true,
      "devDependencies": false,
      "regExps": [
        "\\D$"
      ]
    }
  ],
  "dependencies": {
    "@changesets/cli": "^2.22.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@semantic-release/npm": "^9.0.1",
    "babel-jest": "^27.5.1",
    "babel-preset-gatsby": "^2.11.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "nyc": "^15.1.0",
    "react-test-renderer": "^18.0.0"
  },
  "bugs": {
    "url": "https://github.com/nuktpls/gatsby-layout-builder/issues"
  }
}
